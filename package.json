{
  "name": "dino-ui",
  "version": "0.0.4",
  "description": "Personal Vue UI",
  "main": "dist/index.js",
  "scripts": {
    "start": "parcel index.html --no-cache --open",
    "build": "parcel build index.js --no-cache --no-minify --no-source-maps",
    "dev-test": "parcel watch tests/* --no-cache & karma start",
    "test": "parcel build tests/* --no-cache --no-minify && karma start --single-run",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yancongwen/dino-ui.git"
  },
  "keywords": [
    "vue",
    "ui"
  ],
  "author": "Javon Yan",
  "license": "ISC",
  "dependencies": {
    "vue": "^2.5.21",
    "vue-hot-reload-api": "^2.3.1"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^2.5.0",
    "chai": "^4.2.0",
    "chai-spies": "^1.0.0",
    "karma": "^6.3.14",
    "karma-chai": "^0.1.0",
    "karma-chai-spies": "^0.1.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-sinon-chai": "^2.0.2",
    "mocha": "^5.2.0",
    "node-sass": "^4.11.0",
    "parcel-bundler": "^1.11.0",
    "sass": "^1.16.1",
    "sass-loader": "^7.1.0",
    "sinon": "^7.2.2",
    "sinon-chai": "^3.3.0",
    "vue-template-compiler": "^2.5.21",
    "vuepress": "^0.14.9"
  },
  "alias": {
    "vue": "./node_modules/vue/dist/vue.common.js"
  }
}
